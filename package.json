{
  "name": "vscode-changesets",
  "displayName": "Changesets",
  "description": "A VSCode extension for the changesets workflow.",
  "version": "0.2.0",
  "publisher": "GreatSoftwareLLC",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "changesets",
    "versioning",
    "release",
    "changelog",
    "semver"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gunn4r/vscode-changesets.git"
  },
  "bugs": {
    "url": "https://github.com/gunn4r/vscode-changesets/issues"
  },
  "homepage": "https://github.com/gunn4r/vscode-changesets#readme",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "changeset.add",
        "title": "Changeset: Add (Manual)"
      },
      {
        "command": "changeset.addWithAI",
        "title": "Changeset: Add with AI"
      },
      {
        "command": "changeset.addEmpty",
        "title": "Changeset: Add Empty"
      },
      {
        "command": "changeset.clearApiKey",
        "title": "Changeset: Clear Gemini API Key"
      },
      {
        "command": "changeset.setApiKey",
        "title": "Changeset: Set Gemini API Key"
      }
    ]
  },
  "scripts": {
    "version": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/glob": "^8.1.0",
    "@types/node": "20.x",
    "@types/vscode": "^1.89.0",
    "glob": "^10.4.1"
  },
  "packageManager": "npm@11.4.2"
}
